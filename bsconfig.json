{
  "name": "misthos",
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [{
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "bs-dependencies": [
    "reason-react",
    "bs-json",
    "bs-fetch",
    "@jsiebern/bs-material-ui",
    "reason-reroute",
    "bs-css",
    "bs-webapi",
  ],
  "bs-dev-dependencies": [
     "@glennsl/bs-jest",
  ],
  "reason": {
    "react-jsx": 2
  },
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "refmt": 3
}
